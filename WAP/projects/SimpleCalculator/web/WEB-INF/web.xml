<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>SimpleCalculatorServlet</servlet-name>
        <servlet-class>mum.cs472.SimpleCalculatorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImprovedCalculatorServlet</servlet-name>
        <servlet-class>mum.cs472.ImprovedCalculatorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImprovedCalculatorServlet2</servlet-name>
        <servlet-class>mum.cs472.ImprovedCalculatorServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SimpleCalculatorServlet</servlet-name>
        <url-pattern>/SimpleCalculator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImprovedCalculatorServlet</servlet-name>
        <url-pattern>/ImprovedCalculator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImprovedCalculatorServlet2</servlet-name>
        <url-pattern>/ImprovedCalculator2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
